<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Lemon</title>
   <link rel="stylesheet" type="text/stylesheet" href="/stylesheets/style.css">
   <link href="http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic" rel="stylesheet" type="text/css">
</head>
<body>
<nav>
<h2>WDI-NYC<span class="light">LEMON</span>
<button>Flip All</button>
</nav>
<div class="container">
  <div class="row">
    <% for(var i = 0; i < students.length; i++){ %>
        <div class="flip-container">
          <div class="flipper">
            <div class="front">
              <img src="/img/<%= students[i].pic %>" width="150px">
            </div>
            <div class="back">
              <div class="name">
                <h1><%= students[i].name %></h1>
              </div>
            </div>
          </div>
        </div>

    <% } %>
   </div>
</div>

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
  <script src="/main.js"></script>
</body>
</html>


